			s2.add(br4);
			s2.add(br5);
			
			sc2.setBrands(s2);
			
			sn.save(sc2);
		
		}*/
		
		Query qr3 = sn.createQuery("from SubCategories where sub_cat_Name='Mens'");
		List<?> l3 = qr3.list();
		Iterator<?> it3 = l3.iterator();
		while(it3.hasNext())
		{
			SubCategories sc3 = (SubCategories) it3.next();
			
			Brands br6 = new Brands();
			br6.setBrandID(769);
			br6.setBrandName("Blue Art");
			
			Brands br7 = new Brands();
			br7.setBrandID(770);
			br7.setBrandName("Being Human");
			
			Set<Brands> s3 = new HashSet<Brands>();
			s3.add(br6);
			s3.add(br7);
			
			sc3.setBrands(s3);
			
			sn.save(sc3);
		
		}
		
		Query qr4 = sn.createQuery("from SubCategories where sub_cat_Name='Womens'");
		List<?> l4 = qr4.list();
		Iterator<?> it4 = l4.iterator();
		while(it4.hasNext())
		{
			SubCategories sc4 = (SubCategories) it4.next();
			
			Brands br8 = new Brands();
			br8.setBrandID(771);
			br8.setBrandName("Crescent");
			
			Brands br9 = new Brands();
			br9.setBrandID(772);
			br9.setBrandName("Max");
			
			Set<Brands> s4 = new HashSet<Brands>();
			s4.add(br8);
			s4.add(br9);
			
			sc4.setBrands(s4);
			
			sn.save(sc4);
		
		}
		
		tr.commit();
		sn.close();
	}

